        <TextBox Name="txtPrFreqOrig" IsReadOnly="True" HorizontalAlignment="Left" Height="23" Margin="355,407,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FFEEECEC"/>
