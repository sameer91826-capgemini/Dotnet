        <TextBox Name="txtAddress" HorizontalAlignment="Left" Height="26" Margin="179,330,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtSmoker" HorizontalAlignment="Left" Height="23" Margin="179,295,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtRelation" HorizontalAlignment="Left" Height="23" Margin="179,257,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtNominee" HorizontalAlignment="Left" Height="23" Margin="179,217,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtGender" HorizontalAlignment="Left" Height="23" Margin="179,182,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtDob" HorizontalAlignment="Left" Height="23" Margin="179,143,0,0" TextWrapping="NoWrap" VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtInsuredAge" HorizontalAlignment="Left" Height="23" Margin="179,106,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtInsuredName" HorizontalAlignment="Left" Height="23" Margin="179,75,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <TextBox Name="txtInsuredNameOrig" IsReadOnly="True" HorizontalAlignment="Left" Height="23" Margin="355,75,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" Background="#FFEEECEC"/>
        <TextBox Name="txtInsuredAgeOrig" IsReadOnly="True" HorizontalAlignment="Left" Height="23" Margin="355,106,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" Background="#FFEEECEC"/>
        <TextBox Name="txtDobOrig" IsReadOnly="True" HorizontalAlignment="Left" Height="23" Margin="355,143,0,0" TextWrapping="NoWrap" VerticalAlignment="Top" Width="120" Background="#FFEEECEC"/>
