        <Button Content="Reject" Name="btnReject" HorizontalAlignment="Left" Margin="532,295,0,0" VerticalAlignment="Top" Width="75" Click="BtnReject_Click" />
        <Label Content="Status :" HorizontalAlignment="Left" Margin="536,125,0,0" VerticalAlignment="Top"/>
        <TextBox IsReadOnly="True" Name="txtStatus" HorizontalAlignment="Left" Height="23" Margin="536,155,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="76" Background="White" />
        <TextBox Name="txtCustnum" HorizontalAlignment="Left" Height="23" IsReadOnly="True" Margin="510,420,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FFEEECEC"/>
