        <TextBox Name="txtPolicyID" HorizontalAlignment="Left" Height="23" Margin="454,33,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" IsReadOnly="True" TextChanged="TxtPolicyID_TextChanged"  Background="#FFEEECEC"/>
