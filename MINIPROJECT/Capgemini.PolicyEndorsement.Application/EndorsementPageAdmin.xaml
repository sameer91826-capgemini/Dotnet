        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Capgemini.PolicyEndorsement.Application"
        mc:Ignorable="d"
        Title="EndorsementPageAdmin" Height="450" Width="800">
    <Grid Background="Lavender">
        <DataGrid Name="dgendorsement" HorizontalAlignment="Left" Height="336" Margin="22,51,0,0" VerticalAlignment="Top" Width="743" ItemsSource="{Binding}" AutoGenerateColumns="True" MouseDoubleClick="Dgendorsement_MouseDoubleClick"/>
        <Label Content="Endorsement Records " HorizontalAlignment="Left" Margin="289,10,0,0" VerticalAlignment="Top" FontWeight="Bold" FontSize="18"/>
    </Grid>
</Window>
